{
  "name": "@mamba/cli",
  "version": "3.0.0-alpha",
  "description": "Tools for creating, running, and deploying POS Mamba SDK projects",
  "author": "Stone Payments",
  "license": "Apache-2.0",
  "main": "build/mamba.js",
  "scripts": {
    "start": "yarn run prepare && yarn run watch",
    "build": "babel src --out-dir build --extensions \".ts\" --source-maps --ignore \"src/**/__mocks__/*\",\"src/**/__tests__/*\"",
    "watch": "tsc --watch",
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf build ./tsconfig.tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "files": [
    "build",
    "bin"
  ],
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "__mocks__",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "resetModules": false
  },
  "bin": {
    "mamba": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stone-payments/pos-mamba-sdk.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "sdk",
    "pos",
    "stone",
    "mamba",
    "svelte"
  ],
  "bugs": {
    "url": "https://github.com/stone-payments/pos-mamba-sdk/issues"
  },
  "homepage": "https://github.com/stone-payments/pos-mamba-sdk/tree/master/packages/cli#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1"
  }
}
