  "description": "Collection of web utilities for Mamba Applications",
  "main": "dist/main.js",
  "source": "src/index.js",
  "scripts": {
    "watch": "cross-env NODE_ENV=development webpack --config ../../tools/webpack/config.dev.js --watch",
