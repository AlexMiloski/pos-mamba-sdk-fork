    "build:dev": "cross-env NODE_ENV=development webpack --config ../../tools/webpack/config.build.js",
    "build:analyze": "cross-env NODE_ENV=production webpack --config ../../tools/webpack/config.analyzer.js",
    "test": "echo \"Error: no test specified\" && exit 1"
