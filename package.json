{
  "name": "mamba-sdk",
  "version": "3.0.0-alpha",
  "description": "SDK for develop POS Mamba web environment",
  "homepage": "https://github.com/stone-payments/pos-mamba-sdk#readme",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/stone-payments/pos-mamba-sdk.git"
  },
  "author": "Stone Payments - Mamba Team",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.6.0"
  },
  "scripts": {
    "lint:js": "eslint \"{packages}/**/*.{js,html,svelte}\""
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "commitizen": "^4.1.2",
    "core-js": "^3.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "deasync": "^0.1.20",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-svelte3": "^2.7.3",
    "git-cz": "^4.6.2",
    "husky": ">=4",
    "lerna": "^3.22.0",
    "lint-staged": "^10.2.0",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "svelte": "^3.21.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.9.3"
  }
}
