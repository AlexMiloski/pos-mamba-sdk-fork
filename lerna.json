{
  "version": "1.0.0-rc.12",
  "command": {
    "publish": {
      "ignoreChanges": [
        "*.md",
        "*.json",
        "*.txt",
        "test/**",
        "package.json"
      ]
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  },
  "packages": [
    "packages/cli",
    "packages/components/*",
    "packages/configs",
    "packages/pos",
    "packages/store",
    "packages/styles",
    "packages/webpack"
  ]
}
